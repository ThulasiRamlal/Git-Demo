package com.crud.entity;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.Id;

@Entity
public class Student {
	
    @Id
    @Column
	private int id;
    @Column
	private String name;
    @Column
	private int age;
    // Default Constructor
	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}
    
   
    
   

}
