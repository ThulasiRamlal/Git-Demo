package com.entity;

public class Employee {
	
	
	
	//pozo class
	private int id;
	private String name;
	private int age;
	
	//Setters
	public void setId(int id)
	{
		this.id=id;
	}
	public void setName(String name)
	{
		this.name=name;
	}
	public void setAge(int age)
	{
		this.age=age;
	}
	
//Getters
	
	public int getId()
	{
		return id;
	}
	public String getName()
	{
		return name;
	}
	
	public int getAge()
	{
		return age;
	}
	
	
	
}
